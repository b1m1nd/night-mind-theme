{
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": "eslintrecommended",
  "rules": {
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "semiTWO": {
      "name": "Storage type",
      "scope": "storage.type.function.arrow.js"
    },
    "settings": {
      "foreground": "#dc8cff",
      "fontStyle": "normal"
    }
  }
}
