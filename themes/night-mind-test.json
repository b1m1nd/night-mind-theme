      "settings": {
        "foreground": "#AAE682"
      }
    },

    {
      "name": "String Quoted",
      "scope": ["string.quoted", "string.quoted.single.js"],
      "settings": {
        "foreground": "#AAE682"
      }
    },

    {
      "name": "Backtics(``) in Template Strings",
