{
  "name": "Night Mind Test",
  "type": "dark",
  "include": "./night_defaults.json",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#1B1B1B"
      }
    },
    {
      "name": "normalize font style of certain components",
      "scope": [
        "meta.property-list.css meta.property-value.css variable.other.less",
        "meta.property-list.scss variable.scss",
        "meta.property-list.sass variable.sass",
        "meta.brace",
        "keyword.operator.operator",
        "keyword.operator.or.regexp",
        "keyword.operator.expression.in",
        "keyword.operator.relational",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.type",
        "keyword.operator",
        "keyword",
        "punctuation.definintion.string",
        "punctuation",
        "storage.type",
        "source.css",
        "string.quoted"
      ],
      "settings": {
        "fontStyle": "normal"
      }
    },

    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.git",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#9CD9FF",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Deleted",
      "scope": "markup.deleted.diff",
      "settings": {
        "foreground": "#F05050",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Inserted",
      "scope": "markup.inserted.diff",
      "settings": {
        "foreground": "#A1D884",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#00B1FF"
      }
    },

    {
      "name": "Constant Character Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#AAE682"
      }
    },

    {
      "name": "Meta Tag",
      "scope": ["punctuation.definition.tag", "meta.tag"],
      "settings": {
        "foreground": "#CECECE"
      }
    },

    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e7ca8d",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Pug Classes",
      "scope": "entity.other.attribute-name.class.pug",
      "settings": {
        "foreground": "#e7ca8d"
      }
    },

    {
      "name": "Support Variable Property",
      "scope": "support.variable.property",
      "settings": {
        "foreground": "#64b4fa"
      }
    },

    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F05050",
        "background": "#392020"
      }
    },

    {
      "name": "RegExp String",
      "scope": [
        "string.regexp",
        "string.regexp keyword.other",
        "string.regexp punctuation.definition.string",
        "constant.character.escape.backslash.regexp",
        "constant.other.character-class.set.regexp",
        "constant.character.control.regexp"
      ],
      "settings": {
        "foreground": "#DBD4BA"
      }
    },

    {
      "name": "Constant Other Color",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#CECECE"
      }
    },

    {
      "name": "Italics",
      "scope": "italic",
      "settings": {
        "foreground": "",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Bold",
      "scope": "bold",
      "settings": {
        "foreground": "#AAE682",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Quote",
      "scope": "quote",
      "settings": {
        "foreground": "#AAE682",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#F05050",
        "foreground": "#FFFFFF"
      }
    },

    {
      "name": "Library class/type",
      "scope": [
        "support.constant.json",
        "support.type",
        "support.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#f9826c",
        "fontStyle": "normal"
      }
    },

    {
      "name": "Library (function & constant)",
      "scope": ["support.function", "support.constant"],
      "settings": {
        "foreground": "#9696FF"
      }
    },

    {
      "name": "Variable Property Other object property",
      "scope": ["variable.other.object.property"],
      "settings": {
        "foreground": "#e0b96b"
      }
    },

    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#e0b96b"
      }
    },

    {
      "name": "Support Constant Math",
      "scope": "support.constant.math",
      "settings": {
        "foreground": "#ff7878"
      }
    },

    {
      "name": "Storage type",
      "scope": [
        "storage.type",
        "punctuation.definition.block.tag",
        "meta.method.declaration",
        "meta.method.declaration storage.type.js"
      ],
      "settings": {
        "foreground": "#64B4FA",
        "fontStyle": "italic"
      }
    },

    {
      "name": "interface",
      "scope": ["storage.type.interface"],
      "settings": {
        "foreground": "#64B4FA"
      }
    },

    {
      "name": "Storage",
      "scope": [
        "storage",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "storage.type.property.js",
        "storage.type.property.ts",
        "storage.type.property.tsx"
      ],
      "settings": {
        "foreground": "#64B4FA",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Entity Name Tag Custom",
      "scope": "entity.name.tag.custom",
      "settings": {
        "foreground": "#DC8CFF"
      }
    },

    {
      "name": "Meta Delimiter Period",
      "scope": "meta.delimiter.period",
      "settings": {
        "foreground": "#DC8CFF",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Meta Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#DC8CFF",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Doctypes",
      "scope": ["entity.name.tag.doctype", "meta.tag.sgml.doctype"],
      "settings": {
        "foreground": "#DC8CFF",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Keyword Control Conditional",
      "scope": [
        "keyword.control.conditional.js",
        "keyword.control.conditional.ts",
        "keyword.control.switch.js",
        "keyword.control.switch.ts"
      ],
      "settings": {
        "foreground": "#DC8CFF",
        "fontStyle": "normal"
      }
    },

    {
      "name": "loops",
      "scope": ["keyword.control.loop"],
      "settings": {
        "foreground": "#DC8CFF"
      }
    },

    {
      "name": "import control keyword",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#9696FF"
      }
    },

    {
      "name": "Variable Parameter Function",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#64b4fa"
      }
    },

    {
      "name": "function call",
      "scope": "meta.function-call",
      "settings": {
        "fontStyle": "italic"
      }
    },

    {
      "name": "Meta Property Name",
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#00b1ff"
      }
    },

    {
      "name": "Keyword Control Operator",
      "scope": ["keyword.control.operator", "keyword.control.export"],
      "settings": {
        "foreground": "#ff7878"
      }
    },
    {
      "name": "Functions & Classes",
      "scope": [
        "support.function.json.js",
        "storage.type.function.arrow",
        "entity.name.function",
        "meta.function.call.js",
        "meta.class entity.name.type.class.js",
        "entity.other.inherited-class.js",
        "meta.class entity.name.type.class.tsx",
        "entity.name.type.class.cpp",
        "entity.name.type.inherited.cpp"
      ],
      "settings": {
        "foreground": "#9696ff",
        "fontStyle": "normal"
